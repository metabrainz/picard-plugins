               </widget>
              </item>
              <item>
               <widget class="QCheckBox" name="cwp_collections">
                <property name="toolTip">
                 <string>&lt;html&gt;&lt;head/&gt;&lt;body&gt;&lt;p&gt;This will include parent works where the relationship has the attribute 'part of collection'.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;</string>
                </property>
